unimplemented = true

// Test: type reference completion
// From: crates/eure-editor-support/tests/completions_test.rs:test_type_reference_completion
// Completion of type references after .$types.

schema = ```eure
$types.Person {
    @ name
    $type = .text

    @ age
    $type = .number
}

$types.Address {
    @ street
    $type = .text

    @ city
    $type = .text

    @ country
    $type = .text
}

$types.Company {
    @ name
    $type = .text

    @ employees {
        $array = .$types.Person
    }

    @ headquarters
    $type = .$types.Address
}

@ user {
    # Type will be assigned here
}
```

editor = ```eure
@ user
$type = .$types.|_|
```

trigger = "."

// Should have type names: Person, Address, Company

@ completions[]
label = "Person"
kind = "type"

@ completions[]
label = "Address"
kind = "type"

@ completions[]
label = "Company"
kind = "type"
