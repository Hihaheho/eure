unimplemented = true

// Test: section snippet generation
// From: crates/eure-editor-support/tests/completions_test.rs:test_section_snippet_generation
// Fields with $prefer.section = true generate snippets

schema = ```eure
user.$prefer.section = true
user.name.first.$type = .text
user.name.last.$type = .text
user.name.middle.$type = .text
user.name.middle.$optional = true
user.age.$type = .number
```

editor = ```eure
user.|_|
```

trigger = "."

@ completions[]
label = "name"
kind = "field"
insertTextFormat = "snippet"
// Snippet includes required fields (first, last) but not optional (middle)
// Snippet ends with $0 for final cursor position
insertText = ```
@ user.name
first = ${1:!}
last = ${2:!}
$0
```
result = ```eure
@ user.name
first = |1|
last = |2|
|_|
```
