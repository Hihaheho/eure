unimplemented = true

// Test: completions come from schema
// From: crates/eure-editor-support/tests/no_hardcoded_completions_test.rs:test_completions_come_from_schema

schema = ```eure
@ myfield {
    @ subfield1
    $type = .text

    @ subfield2
    $type = .number
}
```

editor = ```eure
@ myfield.|_|
```

trigger = "."

// Should have exactly 2 completions from schema

@ completions[]
label = "subfield1"
kind = "field"

@ completions[]
label = "subfield2"
kind = "field"

// Should not have any hardcoded values
completions_should_not_contain = ["commands", "options"]
