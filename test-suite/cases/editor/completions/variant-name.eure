unimplemented = true

// Test: variant name completion
// From: crates/eure-editor-support/tests/completions_test.rs:test_variant_name_completion
// Completion of variant names after $variant:

schema = ```eure
$types.Action {
    @ $variants.set-text {
        @ speaker
        $type = .text

        @ lines {
            $array = .text
        }
    }

    @ $variants.set-choices {
        @ description
        $type = .text

        @ choices.$array {
            @ text
            $type = .text

            @ value
            $type = .text
        }
    }

    @ $variants.navigate {
        @ target
        $type = .text

        @ mode
        $type = .text
        $enum = ["push", "replace", "pop"]
    }
}

@ actions {
    $array = .$types.Action
}
```

editor = ```eure
@ actions[]
$variant: |_|
```

trigger = ":"

// Should have variant names: set-text, set-choices, navigate

@ completions[]
label = "set-text"
kind = "variant"

@ completions[]
label = "set-choices"
kind = "variant"

@ completions[]
label = "navigate"
kind = "variant"
