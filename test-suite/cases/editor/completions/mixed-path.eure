// Completion for complex paths like config.servers[].

schema = ```eure
$types.Server {
    @ host
    $type = .text

    @ port
    $type = .number

    @ protocol
    $type = .text
    $enum = ["http", "https", "ws", "wss"]

    @ enabled
    $type = .boolean
    $default = true
}

@ config {
    @ servers {
        $array = .$types.Server
    }
}
```

editor = ```eure
@ config.servers[]
|_|
