input_eure = ```eure
a = { x => 1 }
b.x = 2
f = { = "hello", $ext => "ext" }
g = {}
@ c = { x => 3 }
@ d {
  x = 4
}
@ e
x: 6
empty:
```

normalized = ```eure
= {
  a => { x => 1 }
  b => { x => 2 }
  c => { x => 3 }
  d => { x => 4 }
  e => { x => "6", empty => "" }
  f => "hello"
  f.$ext => "ext"
  g => {}
}
```

output_json = json`{ "a": { "x": 1 }, "b": { "x": 2 }, "c": { "x": 3 }, "d": { "x": 4 }, "e": { "x": "6", "empty": "" }, "f": "hello", "g": {} }`

@cases.integer-keys

input_eure = ```eure
a = { 1 => "one", 2 => "two" }
```

normalized = ```eure
= {
  a => { 1 => "one", 2 => "two" },
}
```

output_json = json`{ "a": { "1": "one", "2": "two" } }`

@cases.middle-integer-keys

input_eure = ```eure
a.1.x = "one"
a.2.y = "two"
a.3.1 = "three"
```

normalized = ```eure
= {
  a => { 1 => { x => "one" }, 2 => { y => "two" }, 3 => { 1 => "three" } },
}
```

output_json = json`{ "a": { "1": { "x": "one" }, "2": { "y": "two" }, "3": { "1": "three" } } }`

