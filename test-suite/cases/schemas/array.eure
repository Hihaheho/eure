// Test cases for array type schemas

// ============================================================================
// Input Data
// ============================================================================

input_eure = ```eure
a = [1, 2, 3]
b = [1, 2, 3]
c = [[1, 2], [3, 4]]
d = [{ a = 1, b = 2 }, { a = 3, b = 4 }]
e = [{ array = [1, 2, 3] }, { array = [4, 5, 6] }]
```

// ============================================================================
// Schema Definition
// ============================================================================

schema = ```eure
a = [.integer]
b = [.integer]
c = [[.integer]]
d = [{ a = .integer, b = .integer }]
e = [{ array = [.integer] }]
```

// ============================================================================
// Expected Output
// ============================================================================

expected_json_schema = ```json
{
  "a": {
    "type": "array",
    "items": { "type": "integer" },
    "minItems": 1,
    "maxItems": 3,
    "uniqueItems": true,
    "contains": { "const": 1 }
  },
  "b": {
    "type": "array",
    "items": { "type": "integer" }
  },
  "c": {
    "type": "array",
    "items": {
      "type": "array",
      "items": { "type": "integer" }
    }
  },
  "d": {
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "a": { "type": "integer" },
        "b": { "type": "integer" }
      },
      "required": ["a", "b"]
    }
  },
  "e": {
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "array": {
          "type": "array",
          "items": { "type": "integer" }
        }
      },
      "required": ["array"]
    }
  }
}
```
